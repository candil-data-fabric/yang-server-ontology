#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/yang/server#accessDatastore
ys:accessDatastore rdf:type owl:ObjectProperty ;
                   rdfs:domain ys:YangServer ;
                   rdfs:range ys:Datastore ;
                   rdfs:comment "Indicates the YANG datastore to be selected when accessing the YANG data of a YANG server." ;
                   rdfs:isDefinedBy <https://w3id.org/yang/server#> ;
                   rdfs:label "access datastore" .


###  https://w3id.org/yang/server#capability
ys:capability rdf:type owl:ObjectProperty ;
              rdfs:domain ys:NetconfServer ;
              rdfs:range ys:NetconfCapability ;
              rdfs:comment "Refers to the NETCONF capability supported by the NETCONF server." ;
              rdfs:isDefinedBy <https://w3id.org/yang/server#> ;
              rdfs:label "capability" .


###  https://w3id.org/yang/server#datastore
ys:datastore rdf:type owl:ObjectProperty ;
             rdfs:domain ys:YangServer ;
             rdfs:range ys:Datastore ;
             rdfs:comment "Indicates the YANG datastore used by the YANG server." ;
             rdfs:isDefinedBy <https://w3id.org/yang/server#> ;
             rdfs:label "datastore" .


###  https://w3id.org/yang/server#profile
ys:profile rdf:type owl:ObjectProperty ;
           rdfs:domain ys:NetconfServer ;
           rdfs:range ys:NetconfProfile ;
           rdfs:comment "Refers to the network profile used when accessing the YANG server via NETCONF protocol." ;
           rdfs:isDefinedBy <https://w3id.org/yang/server#> ;
           rdfs:label "profile" .


###  https://w3id.org/yang/server#vendor
ys:vendor rdf:type owl:ObjectProperty ;
          rdfs:domain ys:NetconfProfile ;
          rdfs:range ys:Vendor ;
          rdfs:comment "Refers to the network vendor that defines the network profile." ;
          rdfs:isDefinedBy <https://w3id.org/yang/server#> ;
          rdfs:label "vendor" .


