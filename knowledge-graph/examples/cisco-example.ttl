
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ys: <https://w3id.org/yang/server#> .
@base <https://example.org/network/> .

<devices/cisco-r1> a ys:NetconfServer ;
    ys:endpoint "192.168.1.10:830" ;
    ys:username "cisco" ;
    ys:password "admin" ;
    ys:hostKeyVerification "false" ;
    ys:profile <networkProfiles/Cisco/IOS-XR> ;
    ys:capability <netconfCapabilities/yang-library:1.1> .

<networkProfiles/Cisco/IOS-XR> a ys:NetconfProfile ;
    dcterms:name "iosxr" ;
    ys:vendor <vendors/Cisco> .

<vendors/Cisco> a ys:Vendor ;
    dcterms:name "Cisco" .

<netconfCapabilities/yang-library:1.1> a ys:NetconfCapability ;
    dcterms:identifier "urn:ietf:params:netconf:capability:yang-library:1.1" .




