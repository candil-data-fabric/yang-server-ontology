        <rdfs:comment>XPath expression used to access YANG data of the YANG server.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/yang/server"/>
        <rdfs:label>xpath value</rdfs:label>
    </owl:DatatypeProperty>
    


