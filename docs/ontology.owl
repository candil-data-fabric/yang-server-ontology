                    <rdf:Description rdf:about="https://w3id.org/yang/server#Subscription"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/yang/server#YangServer"/>
